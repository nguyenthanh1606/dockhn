<configuration>
   <system.webServer>
      <httpRedirect enabled="true" exactDestination="true" httpResponseStatus="Found">
        <add wildcard="*.*" destination="/index.html" />
      </httpRedirect>
        <staticContent>
            <mimeMap fileExtension=".md" mimeType="text/x-markdown" />
        </staticContent>
   </system.webServer>
</configuration>